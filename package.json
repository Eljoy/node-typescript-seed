{
  "name": "node-typescript-seed",
  "version": "0.0.1",
  "description": "A simple Node/TypeScript seed with watch support.",
  "main": "src/app/app.ts",
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node ./build/app/app.js",
    "predev": "npm run clean",
    "dev": "concurrently \"npm run tsc-watch\" \"npm run nodemon\"",
    "prenodemon": "wait-on ./build/app/app.js",
    "nodemon": "nodemon --delay 1500ms \"./build/app/app.js\"",
    "tsc-watch": "tsc -w",
    "lint": "tslint -c tslint.json \"./src/**/*.ts\""
  },
  "author": "Andre Giannico <andre@andregiannico.com>",
  "license": "MIT",
  "dependencies": {
    "@nlpjs/basic": "^4.26.1",
    "@papercups-io/papercups": "^1.0.4",
    "@types/express": "^4.17.15",
    "@types/node": "^16.13.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "paper-cups": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.5.4",
    "tslint": "^3.15.1",
    "typescript": "^4.9.4",
    "wait-on": "^1.5.3"
  }
}
